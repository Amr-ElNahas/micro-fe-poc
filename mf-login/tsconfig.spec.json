{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/spec",
    "types": ["jasmine"],
    "paths": {

      "@SUMERGE/*": ["./src/app/services/@SUMERGE/*"],
      "@store/*": ["./src/assets/store/*"],
      "@loginServices/*":["./src/app/services/*"],
      "@models/*":["./src/app/models/*"]
